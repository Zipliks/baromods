        <StatusEffect type="OnUse" target="UseTarget">
          <Conditional entitytype="eq Character" />
          <Sound file="Content/Items/Weapons/Smack1.ogg" range="500" />
          <Sound file="%ModDir%/Sounds/ss_sparks.ogg" range="500" volume="1" />
        </StatusEffect>
